    "@mui/material": "^5.14.18",
    "@reduxjs/toolkit": "^1.9.7",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.19.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
